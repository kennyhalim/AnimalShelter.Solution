@using AnimalShelter.Models

<h1> List of Animals </h1>

@if (Model.Count == 0)
{
<p> You have no animals yet </p>
}

@if (Model.Count != 0)
{
<a href='/animal/name'>name</a>    <a href='/animal/sex'>sex</a>   <a href='/animal/breed'>breed</a>   <a href='/animal/dateofadmittance'>date of admittance</a>
@foreach (Animal oneAnimal in Model)
{
<p>@oneAnimal.GetName()   @oneAnimal.GetSex()    @oneAnimal.GetBreed()    @oneAnimal.GetDateOfAdmittance()</p>
}
}
